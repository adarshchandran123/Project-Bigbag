                  <h3 style="font-weight: 900;font-size: 40px; color: white;" class="card-title">{{allorderStatus.totalOrder}}</h3>
                </div>
                <div class="card-footer">
                  <div class="stats">
                    {{!-- <i class="material-icons">update</i> Just Updated --}}
                  </div>
                </div>
              </div>
            </div>
          </div>







  <div class="row">
    <div style="margin-top: 50px;"  class="col-md-6">
      
      <canvas id="canvas"></canvas>
    </div>
    <div style="margin-top: 50px;"  class="col-md-6">
      <canvas id="payment"></canvas>
    </div>
    <div style="margin-top: 50px;"  class="col-md-6">
      <canvas id="piedDiagram"></canvas>
    </div>
    
    <div style="margin-top: 50px;"  class="col-md-6">

      {{!-- <canvas id="order"></canvas> --}}
      
    </div>
    <div style="margin-top: 50px;"  class="col-md-12">

      <p style="font-size: 40px; color: white; font-family: 'Acme', sans-serif;">Latest Product</p>

      <table style="background-color: rgb(3 12 35); border: solid 2px rgb(20, 88, 90);" class="table table-hover">
  <thead>
    <tr>
      <th style="color: white; font-weight: bolder;" scope="col">No</th>
      <th style="color: white; font-weight: bolder;" scope="col"> </th>
      <th style="color: white; font-weight: bolder;" scope="col">Name</th>
      <th style="color: white; font-weight: bolder;" scope="col">Category</th>
      <th style="color: white; font-weight: bolder;" scope="col">Quantity</th>
      <th style="color: white; font-weight: bolder;" scope="col">MRP</th>
      <th style="color: white; font-weight: bolder;" scope="col">Offer %</th>
      <th style="color: white; font-weight: bolder;" scope="col">Price </th>
    </tr>
  </thead>
  <tbody >
    {{#each arrival}}
    <tr style="border: solid 2px rgb(20, 88, 90);  ">
      <th style="color: white;" scope="row">{{add @index '1'}}</th>
      <td style="color: white;"><img width="70px" height="70px" src="/product-image/{{this._id}}__1.jpg"></td>
      <td style="color: white;">{{this.productName}}</td>
      <td style="color: white;">{{this.category}}</td>
      <td style="color: white;">{{this.qty}}</td>
      <td style="color: white;">{{this.MRP}}</td>
      <td style="color: white;">{{this.offer}}</td>
      <td style="color: white;">{{this.newprice}}</td>
     
    </tr>
    {{/each}}
  </tbody>
</table>


      

    </div>

    
  </div>
</div>




<style>
  #canvas{
    width: 100%;
   
    
  },
  #piedDiagram{
    width: 100%;
    
    
  },
  #payment{
    width: 100%;
    
    
  },
    #order{
    width: 100%;
    
    
  }


</style>



<script src="https://cdn.jsdelivr.net/npm/chart.js@3.5.1/dist/chart.min.js"></script>




<script>
const ctx = document.getElementById('canvas').getContext('2d');
const myChart = new Chart(ctx, {
    type: 'bar',
    data: {
        labels: ['Cancelled', 'Pending', 'Delivered', 'Placed', 'Shipped'],
        datasets: [{
            label: '# Order Status',
            data: [{{lineorder}}],
            backgroundColor: [
                'rgba(255, 99, 132, 0.2)',
                'rgba(54, 162, 235, 0.2)',
                'rgba(255, 206, 86, 0.2)',
                'rgba(75, 192, 192, 0.2)',
                'rgba(153, 102, 255, 0.2)',
                'rgba(255, 159, 64, 0.2)'
            ],
            borderColor: [
                'rgba(255, 99, 132, 1)',
                'rgba(54, 162, 235, 1)',
                'rgba(255, 206, 86, 1)',
                'rgba(75, 192, 192, 1)',
                'rgba(153, 102, 255, 1)',
                'rgba(255, 159, 64, 1)'
            ],
            borderWidth: 1
        }]
    },
    options: {
        scales: {
            y: {
                beginAtZero: true
            }
        }
    }
})
</script>




