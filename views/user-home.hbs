<!-- ========================= SECTION  ========================= -->
<section class="section-name padding-y-sm">
<div class="container">

<header class="section-heading">
	<a href="/userAllProduct" class="btn btn-outline-primary float-right">See all</a>
	<h3 class="section-title">New arrival</h3>
</header><!-- sect-heading -->

	
<div class="row">
  {{#each arrival}}
	<div class="col-sm-12 col-md-6 col-lg-3">

		<div class="card card-product-grid">
			<a href="/product-detail?id={{this._id}}" class="img-wrap"> <img src="/product-image/{{this._id}}__1.jpg"> </a>
			<figcaption class="info-wrap">
				<a style="font-weight: bolder;" href="./product-detail.html" class="title">{{this.productName}}</a>
				<a href="./product-detail.html" class="title">{{this.discription}}</a>
				<div class="price mt-1">₹ {{this.newprice}} <span style="padding-left: 10px; color: darkgray;">₹ <s>{{this.oldprice}}</s></span></div> <!-- price-wrap.// -->
				<div>
					<a style="margin-top: 10px;" class="btn btn-success" href="">Buy Now</a>
					<a style="margin-top: 10px;" class="btn btn-primary" href="">Add to Cart <i class="fas fa-shopping-cart"></i></a>
				</div>
			</figcaption>
		</div>

	</div> 
  {{/each }}
  
 
	
</div> <!-- row.// -->

</div><!-- container // -->
</section>
<!-- ========================= SECTION  END// ========================= -->


















